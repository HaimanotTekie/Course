    /**
     * Internal use.
     * Inits the dialog. 
     */
    public void init() {        
        totalBlockAllEvents++;
        
        /* Her kommer generert kode fra WindowBlock:SetupDialogPane*/
        
        setTitle("Course");
        getContentPane().add("Center",Window_outer);
        setFrameIcon(RepositoryProxy.getImageField("DefaultIcon"));
        
        /* Her kommer generert kode fra WindowBlock:SetupDialogPane*/
        
        setTitle("Course");
        getContentPane().add("Center",Window2_outer);
        setFrameIcon(RepositoryProxy.getImageField("DefaultIcon"));
        
        /* Her kommer generert kode fra WindowBlock:SetupDialogPane*/
        
        setTitle("Course");
        getContentPane().add("Center",Window3_outer);
        setFrameIcon(RepositoryProxy.getImageField("DefaultIcon"));
        initOuterPanel();
        initNameComponentMappings();
        initFocusActions();
        pack();
        getRootPane().putClientProperty("defeatSystemEventQueueCheck", Boolean.TRUE);
        controller.application.commonViewStartup(this, controller);
        totalBlockAllEvents--;
    }


